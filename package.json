{
  "name": "redink-graphql",
  "version": "0.0.1-alpha.0",
  "description": "GraphQL utilities for Redink",
  "repository": "directlyio/redink-graphql",
  "main": "index.js",
  "author": "Dylan Slack <dylanslack@gmail.com> (http://www.dylanslack.me)",
  "license": "MIT",
  "scripts": {
    "build": "npm run clean && mkdirp dist && babel src --out-dir dist",
    "docs": "babel-node documentation",
    "clean": "rimraf dist coverage .nyc_output",
    "cover": "npm run clean && nyc ava && nyc report --reporter=html && open -a \"Google Chrome\" coverage/index.html",
    "test": "npm run test:lint && npm run test:src",
    "test:lint": "eslint src test",
    "test:src": "nyc ava && nyc report --reporter=lcov",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "graphql": "^0.7.2"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "eslint": "^3.0.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.10.3",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^5.2.2",
    "express": "^4.14.0",
    "express-graphql": "^0.5.4",
    "redink": "next",
    "redink-schema": "0.0.3"
  },
  "ava": {
    "files": [
      "test"
    ],
    "source": [
      "src/**/*.{js}",
      "!dist/**/*"
    ],
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
